# FollowBench Evaluation Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for GPT-4 inference and evaluation)
OPENAI_API_KEY=

# Azure OpenAI (optional - use with --azure flag)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_DEPLOYMENT=gpt-4
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# DeepSeek API (optional - for deepseek-v3 model)
DEEPSEEK_API_KEY=

# Qwen/DashScope API (optional - for qwen-max model)
DASHSCOPE_API_KEY=
